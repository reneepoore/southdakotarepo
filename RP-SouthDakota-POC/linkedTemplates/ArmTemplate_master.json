{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RP-SouthDakota-POC"
		},
		"AdventureWorksLT2019_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdventureWorksLT2019'"
		},
		"SouthDakotaGitHub_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'SouthDakotaGitHub'"
		},
		"SouthDakotaGitHub_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "reneepoore"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RP-SouthDakota-POC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorksLT2019_connectionString": {
						"value": "[parameters('AdventureWorksLT2019_connectionString')]"
					},
					"SouthDakotaGitHub_credential": {
						"value": "[parameters('SouthDakotaGitHub_credential')]"
					},
					"SouthDakotaGitHub_properties_typeProperties_username": {
						"value": "[parameters('SouthDakotaGitHub_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}