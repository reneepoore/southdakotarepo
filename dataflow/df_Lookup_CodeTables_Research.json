{
	"name": "df_Lookup_CodeTables_Research",
	"properties": {
		"folder": {
			"name": "Testing"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CaseHeader",
						"type": "DatasetReference"
					},
					"name": "srcCaseHeader"
				},
				{
					"dataset": {
						"referenceName": "CodeTables",
						"type": "DatasetReference"
					},
					"name": "srcLookupCodeTables"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "CasePriority"
				},
				{
					"name": "CaseOutcome"
				},
				{
					"name": "CaseStatus"
				},
				{
					"name": "CaseObjective"
				},
				{
					"name": "CaseTypeCode"
				},
				{
					"name": "CaseClassification"
				},
				{
					"name": "CaseCatTypeCode"
				}
			],
			"scriptLines": [
				"source(output(",
				"          APPEALINDICATOR as string,",
				"          CASEID as long,",
				"          INTEGRATEDCASEID as long,",
				"          CASEREFERENCE as string,",
				"          CASETYPECODE as string,",
				"          CLASSIFICATIONCODE as string,",
				"          EXPECTEDOUTCOME as long,",
				"          ACTUALOUTCOME as string,",
				"          OWNERORGOBJECTLINKID as long,",
				"          VERSIONNO as integer,",
				"          LASTWRITTEN as timestamp,",
				"          INTEGRATEDCASETYPE as string,",
				"          OBJECTIVECODE as string,",
				"          OUTCOMECODE as string,",
				"          PLANID as long,",
				"          PRIORITYCODE as string,",
				"          RECEIVEDDATE as date,",
				"          EFFECTIVEDATE as date,",
				"          STATUSCODE as string,",
				"          ENDDATE as date,",
				"          EXPECTEDENDDATE as date,",
				"          EXPECTEDSTARTDATE as date,",
				"          FIRSTREVIEWDATE as date,",
				"          COMMENTS as string,",
				"          CONCERNROLEID as long,",
				"          REGISTRATIONDATE as date,",
				"          STARTDATE as date,",
				"          DEFAULTCURRENCYTYPECODE as string,",
				"          DEFAULTDELIVERYMETHODTYPE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> srcCaseHeader",
				"source(output(",
				"          TABLENAME as string,",
				"          CODE as string,",
				"          DESCRIPTION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT ch.TABLENAME,\\n       ci.CODE,\\n       ci.DESCRIPTION\\nFROM   [CURAPD01].[CODETABLEHEADER] ch\\n       JOIN [CURAPD01].[CODETABLEITEM] ci\\n         ON ch.TABLENAME = ci.TABLENAME\\n           Where ch.TABLENAME in (\\n\\n           \\'CaseCatTypeCode\\',\\n           \\'CaseClassification\\',\\n           \\'CaseCatTypeCode\\',\\n           \\'CaseObjective\\',\\n           \\'CaseOutcome\\',\\n           \\'CasePriority\\',\\n           \\'CaseStatus\\')  and LOCALEIDENTIFIER = \\'en\\'',",
				"     format: 'query') ~> srcLookupCodeTables",
				"srcLookupCodeTables filter(TABLENAME=='CasePriority') ~> CasePriority",
				"srcLookupCodeTables filter(TABLENAME=='CaseOutcome') ~> CaseOutcome",
				"srcLookupCodeTables filter(TABLENAME=='CaseStatus') ~> CaseStatus",
				"srcLookupCodeTables filter(TABLENAME=='CaseObjective') ~> CaseObjective",
				"srcLookupCodeTables filter(TABLENAME=='CaseTypeCode') ~> CaseTypeCode",
				"srcLookupCodeTables filter(TABLENAME=='CaseClassification') ~> CaseClassification",
				"srcLookupCodeTables filter(TABLENAME=='CaseCatTypeCode') ~> CaseCatTypeCode"
			]
		}
	}
}