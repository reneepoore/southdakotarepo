{
	"name": "df_Lookup_CodeTables_Research",
	"properties": {
		"folder": {
			"name": "Testing"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CodeTables",
						"type": "DatasetReference"
					},
					"name": "srcCaseHeader"
				},
				{
					"dataset": {
						"referenceName": "CodeTables",
						"type": "DatasetReference"
					},
					"name": "srcLookupCodeTables"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "CasePriority"
				},
				{
					"name": "CaseOutcome"
				},
				{
					"name": "CaseStatus"
				},
				{
					"name": "CaseObjective"
				},
				{
					"name": "CaseTypeCode"
				},
				{
					"name": "CaseClassification"
				},
				{
					"name": "CaseCatTypeCode"
				},
				{
					"name": "filter1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TABLENAME as string,",
				"          CODE as string,",
				"          DESCRIPTION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> srcCaseHeader",
				"source(output(",
				"          TABLENAME as string,",
				"          CODE as string,",
				"          DESCRIPTION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT ch.TABLENAME,\\n       ci.CODE,\\n       ci.DESCRIPTION\\nFROM   [CURAPD01].[CODETABLEHEADER] ch\\n       JOIN [CURAPD01].[CODETABLEITEM] ci\\n         ON ch.TABLENAME = ci.TABLENAME\\n           Where ch.TABLENAME in (\\n\\n           \\'CaseCatTypeCode\\',\\n           \\'CaseClassification\\',\\n           \\'CaseCatTypeCode\\',\\n           \\'CaseObjective\\',\\n           \\'CaseOutcome\\',\\n           \\'CasePriority\\',\\n           \\'CaseStatus\\')  and LOCALEIDENTIFIER = \\'en\\'',",
				"     format: 'query') ~> srcLookupCodeTables",
				"srcLookupCodeTables filter(TABLENAME=='CasePriority') ~> CasePriority",
				"srcLookupCodeTables filter(TABLENAME=='CaseOutcome') ~> CaseOutcome",
				"srcLookupCodeTables filter(TABLENAME=='CaseStatus') ~> CaseStatus",
				"srcLookupCodeTables filter(TABLENAME=='CaseObjective') ~> CaseObjective",
				"srcLookupCodeTables filter(TABLENAME=='CaseTypeCode') ~> CaseTypeCode",
				"srcLookupCodeTables filter(TABLENAME=='CaseClassification') ~> CaseClassification",
				"srcLookupCodeTables filter(CODE=='AT1') ~> CaseCatTypeCode",
				"select1 filter(TABLENAME=='AbsentParentReason') ~> filter1",
				"srcCaseHeader select(mapColumn(",
				"          TABLENAME,",
				"          CODE,",
				"          DESCRIPTION",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}